 static inline void __cons_write_ll(const char *buf, int len) {
+#ifdef __EMSCRIPTEN__
+	// Emscripten: Use EM_ASM to call JavaScript print function directly.
+	// This bypasses the broken write() syscall and uses Module.print.
+	if (len > 0) {
+		char *tmp = malloc(len + 1);
+		if (tmp) {
+			memcpy(tmp, buf, len);
+			tmp[len] = '\0';
+			EM_ASM({ if (Module.print) Module.print(UTF8ToString($0)); }, tmp);
+			free(tmp);
+		}
+	}
+	return;
+#endif
